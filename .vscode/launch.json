{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "${workspaceFolder}/test/test",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/src/ferrum.io",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "LOG_LEVEL",
                    "value": "ALL"
                },
                {
                    "name": "REDIS_HOST",
                    "value": "localhost:6379"
                },
                {
                    "name": "REDIS_LOCAL_HOST",
                    "value": "localhost:6380"
                },
                {
                    "name": "RAW_DESTINATION_HOST",
                    "value": "192.168.88.10"
                },
                {
                    "name": "RAW_DESTINATION_TCP_PORT",
                    "value": "80"
                },
                {
                    "name": "RAW_DESTINATION_UDP_PORT",
                    "value": "5555"
                },
                {
                    "name": "RAW_LISTEN_IP",
                    "value": "192.168.88.10"
                },
                {
                    "name": "RAW_LISTEN_TCP_PORT",
                    "value": "8181"
                },
                {
                    "name": "RAW_LISTEN_UDP_PORT",
                    "value": "8888"
                },
                {
                    "name": "GATEWAY_ID",
                    "value": "gateway1"
                },
                {
                    "name": "SERVICE_ID",
                    "value": "mysqlservice"
                },
                {
                    "name": "INSTANCE_ID",
                    "value": "randominstance"
                },
                {
                    "name": "LD_LIBRARY_PATH",
                    "value": "${workspaceFolder}/external/libs/lib"
                }
            ],
            "additionalSOLibSearchPath": "${workspaceFolder}/external/libs/lib",
            "externalConsole": false,
            "miDebuggerPath": "${workspaceFolder}/sudo.gdb.sh",
            "MIMode": "gdb",
            "preLaunchTask": "buildprod",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Launch Test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/test/ferrum.io.test",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/test",
            "environment": [
                {
                    "name": "LOG_LEVEL",
                    "value": "ALL"
                },
                {
                    "name": "REDIS_HOST",
                    "value": "localhost:6379"
                },
                {
                    "name": "RAW_DESTINATION_HOST",
                    "value": "192.168.88.10"
                },
                {
                    "name": "RAW_DESTINATION_TCP_PORT",
                    "value": "80"
                },
                {
                    "name": "RAW_DESTINATION_UDP_PORT",
                    "value": "5555"
                },
                {
                    "name": "RAW_LISTEN_IP",
                    "value": "192.168.88.10"
                },
                {
                    "name": "RAW_LISTEN_TCP_PORT",
                    "value": "8181"
                },
                {
                    "name": "RAW_LISTEN_UDP_PORT",
                    "value": "8888"
                },
                {
                    "name": "GATEWAY_ID",
                    "value": "gateway1"
                },
                {
                    "name": "SERVICE_ID",
                    "value": "mysqlservice"
                },
                {
                    "name": "INSTANCE_ID",
                    "value": "randominstance"
                },
                {
                    "name": "LD_LIBRARY_PATH",
                    "value": "${workspaceFolder}/external/libs/lib"
                }
            ],
            "externalConsole": false,
            "additionalSOLibSearchPath": "${workspaceFolder}/external/libs/lib",
            "MIMode": "gdb",
            "preLaunchTask": "buildtest",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}